
<div class="col-md-12">
  <table class="table table-hover">

<h2> user 清單</h2>

<thead>
  <tr>
    <td>點數為零的user</td>
    <td>user id</td>
    <td>email</td>
    <td>chip</td>
    <td>最近一次登入</td>
  </tr>
</thead>

<tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name  %></td>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= user.chip %></td>
      <td><%= user.last_sign_in_at %></td>
    </tr>
  <% end %>

</table>
</div>
